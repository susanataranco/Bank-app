{
  "name": "transcations-app",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
  "clean": "rm -rf dist/ frontend-dist/ backend-dist/",
  "build:frontend": "webpack --config webpack.config.mjs --mode production && mkdir -p frontend-dist && mv dist/* frontend-dist/ || echo 'No frontend files to move'",
  "build:backend": "tsc && mkdir -p backend-dist && mv dist/api backend-dist/ || echo 'No backend files to move'",
  "build": "npm run clean && npm run build:frontend && npm run build:backend"
},
